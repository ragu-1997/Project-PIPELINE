{
  "id": "mesos-ui",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "capgemini/mesos-ui:standalone-latest",
      "forcePullImage": false,
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 5000, "protocol": "tcp", "servicePort": 5000}
      ]
    }
  },
  "env": {
    "ZOOKEEPER_ADDRESS": "10.128.1.54:2181,10.128.3.65:2181,10.128.5.100:2181",
    "SERVICE_NAME": "mesos-ui",
    "SERVICE_TAGS": "backend,http"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 120,
      "intervalSeconds": 15,
      "maxConsecutiveFailures": 10,
      "path": "/",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 5
    }
  ],
  "instances": 1,
  "cpus": 1,
  "mem": 512
}
