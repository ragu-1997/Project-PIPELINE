{
  "id": "kafka-manager",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "docker.jangl.com/kafka-manager:1.3.3.16",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        {"containerPort": 9000, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "ZK_HOSTS": "zookeeper.service.consul:2181",
    "SERVICE_NAME": "kafka-web-console",
    "SERVICE_TAGS": "backend,http"
  },
  "instances": 1,
  "cpus": 0.75,
  "mem": 1024
}
