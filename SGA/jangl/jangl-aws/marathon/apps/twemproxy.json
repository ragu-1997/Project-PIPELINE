{
    "id": "/twemproxy",
    "apps": [{
        "id": "/twemproxy/accounts-cache",
        "instances": 1,
        "cpus": 1,
        "mem": 256,
        "container": {
          "type": "DOCKER",
          "docker": {
            "image": "docker.jangl.com/twemproxy-consul",
            "network": "HOST"
          }
        },
        "env": {
            "REDIS_PORT": "7379",
            "REDIS_NAME": "accounts-cache"
        },
        "ports": [7379],
        "requirePorts" : true,
        "healthChecks": [{
            "protocol": "TCP",
            "gracePeriodSeconds": 60,
            "intervalSeconds": 5,
            "port": 7379,
            "timeoutSeconds": 5,
            "maxConsecutiveFailures": 3
        }]
