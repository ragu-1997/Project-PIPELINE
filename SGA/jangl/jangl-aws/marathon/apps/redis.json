{
    "id": "/redis",
    "apps": [{
        "id": "/redis/pingpost-qualifiers",
        "instances": 1,
        "cpus": 1,
        "mem": 1024,
        "container": {
          "type": "DOCKER",
          "docker": {
            "image": "redis:3.2.9",
            "network": "BRIDGE",
            "portMappings": [{"containerPort": 6379, "hostPort": 6379, "protocol": "tcp"}],
            "parameters": [{
                "key": "volume-driver",
                "value": "rexray"
            }, {
                "key": "volume",
                "value": "pingpost-qualifiers-redis:/data"
            }]
          }
        },
        "labels":{
            "MARATHON_SINGLE_INSTANCE_APP": "true"
        },
        "requirePorts" : true,
        "healthChecks": [{
            "protocol": "TCP",
            "gracePeriodSeconds": 60,
            "intervalSeconds": 5,
            "portIndex": 0,
            "timeoutSeconds": 5,
            "maxConsecutiveFailures": 3
        }],
        "upgradeStrategy":{
            "minimumHealthCapacity": 0,
            "maximumOverCapacity": 0
        }
    }, {
